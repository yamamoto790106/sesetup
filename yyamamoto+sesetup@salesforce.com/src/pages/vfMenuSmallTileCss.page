<apex:page showHeader="false" sidebar="false">
    <style type="text/css">
        div.mnucontainer{
            margin-left:auto;
            margin-right:auto;
        }

        div.imgCellBox {
            margin: 10px 10px 10px 10px;
            font-family: Arial, Helvetica, sans-serif;
            font-weight: bold;
            color: #000000;
            font-size: 110%;
        }

        div.imgCellBox ul {
            border: none;
            margin: 0;
            padding: 0;
            float: left;
            clear: both;
        }

        div.imgCellBox p {
            border: none;
            margin: 10px 10px 10px 10px;
            padding: 0;
            font-weight: bold;
            color: #0000ff;
            font-size: 110%;
            float: left;
            clear: both;
        }

        div.imgCellBox li {
            margin: 0;
            padding: 5px;
            float:left;
            width: 128px;
            height:112px;
            border: solid 1px #aaaaaa;
            list-style-type: none;
            vertical-align: top;
            text-align: center;
            overflow: hidden;
            line-height :100%;
        }

        div.imgCellBox li a {
            color:#008800;
            font-weight: bold;
            text-decoration:none;
            overflow: hidden;
            outline: none;
            white-space:normal;
        }

        div.imgCellBox li a:hover{
            color: #ff0000;
            font-weight: bolder;
            text-decoration:none;
        }

        div.imgCellBox li a:hover img{
            opacity:1.0;
            filter: alpha(opacity=100);
            -ms-filter: "alpha( opacity=100 )";
            background: #fff;
        }

        div.imgCellBox li img{
            display:block;
            margin-left:auto;
            margin-right:auto;
            text-align:center;
            margin-bottom:5px;
            width:72px;
        }

        div.imgCellBox li a img{
            opacity:0.6;
            filter: alpha(opacity=60);
            -ms-filter: "alpha( opacity=60 )";
            background: #fff;
        }
    </style>
</apex:page>